<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.alfresco.org/model/dictionary/1.0" name="damsBase:contentModel">
   <!-- Optional meta-data about the model -->
   <description>Base Content model for GNMA DAMS</description>
   <author>Tauqir Rajput</author>
   <version>1.0</version>

   <imports>
      <!-- Import Alfresco Dictionary Definitions -->
      <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
      <!-- Import Alfresco Content Domain Model Definitions -->
      <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
      <!-- Import Alfresco System Model Definitions -->
      <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys" />
   </imports>

   <!-- Custom namespace for the GNMA -->
   <namespaces>
      <namespace uri="http://www.ginniemae.org/model/content/damsBase/1.0" prefix="damsBase" />
   </namespaces>

   <types>
      <!-- Base Content Type for DAMS-->
      <type name="damsBase:damsBaseDocument">
         <title>DAMS Base Content Type</title>
         <parent>cm:content</parent>
      </type>
   </types>
</model>