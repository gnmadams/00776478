<?xml version="1.0" encoding="UTF-8"?>
<model name="oipmroot:contentModel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

    <!-- Optional meta-data about the model -->
    <description>Content model for GNMA DAMS</description>
