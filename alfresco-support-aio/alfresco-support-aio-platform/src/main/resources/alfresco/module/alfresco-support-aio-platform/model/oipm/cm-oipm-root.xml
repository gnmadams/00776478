            <title>OIPM Documents</title>
            <parent>gnmaRoot:document</parent>
            <properties>
                <property name="oipmroot:issuerNumber">
                    <title>Issuer Number</title>
                    <type>d:text</type>
	                <mandatory>true</mandatory>
                    <index enabled="true">
                        <tokenised>TRUE</tokenised>
                        <facetable>false</facetable>
                    </index>
                </property>
                <property name="oipmroot:issuerName">
                    <title>Issuer Name</title>
                    <type>d:text</type>
	                <mandatory>true</mandatory>
                    <index enabled="true">
                        <tokenised>TRUE</tokenised>
                        <facetable>false</facetable>
                    </index>
                </property>
            </properties>
        </type>
    </types>

