<?xml version="1.0" encoding="UTF-8"?>
<model name="smfformreq:contentModel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

    <!-- Optional meta-data about the model -->
    <description>Content model for GNMA DAMS</description>
    <author>Tauqir Rajput</author>
    <version>1.0</version>

    <imports>
        <!-- Import Alfresco Dictionary Definitions -->
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <!-- Import Alfresco Content Domain Model Definitions -->
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
        <!-- Import Alfresco System Model Definitions -->
        <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>
        
        <import uri="http://www.ginniemae.org/model/content/oipmroot/1.0" prefix="oipmroot"/>
        
    </imports>
    
    <namespaces>
        <namespace uri="http://www.ginniemae.org/model/content/smfformreq/1.0" prefix="smfformreq"/>
    </namespaces>

   <constraints>
      <constraint name="smfformreq:subContentTypeList" type="LIST">
         <parameter name="allowedValues">
            <list>
               <value />
               <!-- Empty for default search-->
				<value>FOIA Request</value>
				<value>FOIA Supplemental</value>
            </list>
         </parameter>
      </constraint>
   </constraints>

    <types>
        <type name="smfformreq:smfFormalRequest">
            <title>OIPM Single-Multifamily Formal Request</title>
            <parent>oipmroot:oipmRoot</parent>
            <properties>
                <property name="smfformreq:FoiaIdNum">
                    <title>FOIA ID / Number</title>
                    <type>d:text</type>
                    <index enabled="true">
                        <tokenised>TRUE</tokenised>
                        <facetable>false</facetable>
                    </index>
                </property>
                <property name="smfformreq:subtype">
                    <title>Sub Content Type</title>
                    <type>d:text</type>
                    <index enabled="true">
                        <tokenised>TRUE</tokenised>
                        <facetable>false</facetable>
                    </index>
                    <constraints>
                  		<constraint ref="smfformreq:subContentTypeList" />
               		</constraints>
                </property>
            </properties>
        </type>
    </types>
</model>