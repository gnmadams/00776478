<?xml version="1.0" encoding="UTF-8"?>
<model name="smfcondoc:contentModel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

    <!-- Optional meta-data about the model -->
    <description>Content model for GNMA DAMS</description>
    <author>Tauqir Rajput</author>
    <version>1.0</version>

    <imports>
        <!-- Import Alfresco Dictionary Definitions -->
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <!-- Import Alfresco Content Domain Model Definitions -->
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
        <!-- Import Alfresco System Model Definitions -->
        <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>
        
        <import uri="http://www.ginniemae.org/model/content/oipmcont/1.0" prefix="oipmcont"/>
        
    </imports>
    
    <namespaces>
        <namespace uri="http://www.ginniemae.org/model/content/smfcondoc/1.0" prefix="smfcondoc"/>
    </namespaces>

   <constraints>
      <constraint name="smfcondoc:subContentTypeList" type="LIST">
         <parameter name="allowedValues">
            <list>
               <value />
               <!-- Empty for default search-->
				<value>OIPM-Contract Documents (Specific to a group in GNMA reviewing the various contracts.)</value>
            </list>
         </parameter>
      </constraint>
   </constraints>

    <types>
        <type name="smfcondoc:smfContractDocuments">
            <title>OIPM Single-Multifamily Contract Documents</title>
            <parent>oipmcont:oipmContract</parent>
            <properties>
                <property name="smfcondoc:subtype">
                    <title>Sub Content Type</title>
                    <type>d:text</type>
                    <index enabled="true">
                        <tokenised>TRUE</tokenised>
                        <facetable>false</facetable>
                    </index>
                    <constraints>
                  		<constraint ref="smfcondoc:subContentTypeList" />
               		</constraints>
                </property>
            </properties>
        </type>
    </types>
</model>