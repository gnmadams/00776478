<?xml version="1.0" encoding="UTF-8"?>
<model name="smfscancon:contentModel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

    <!-- Optional meta-data about the model -->
    <description>Content model for GNMA DAMS</description>
    <author>Tauqir Rajput</author>
    <version>1.0</version>

    <imports>
        <!-- Import Alfresco Dictionary Definitions -->
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <!-- Import Alfresco Content Domain Model Definitions -->
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
        <!-- Import Alfresco System Model Definitions -->
        <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>
        
        <import uri="http://www.ginniemae.org/model/content/oipmcont/1.0" prefix="oipmcont"/>
        
    </imports>
    
    <namespaces>
        <namespace uri="http://www.ginniemae.org/model/content/smfscancon/1.0" prefix="smfscancon"/>
    </namespaces>

   <constraints>
      <constraint name="smfscancon:subContentTypeList" type="LIST">
         <parameter name="allowedValues">
            <list>
               <value />
               <!-- Empty for default search-->
				<value>Contract Files</value>
				<value>Contract Custodian Files</value>
            </list>
         </parameter>
      </constraint>
   </constraints>

    <types>
        <type name="smfscancon:smfScannedContractDocuments">
            <title>OIPM Single-Multifamily Scanned Contract Documents</title>
            <parent>oipmcont:oipmContract</parent>
            <properties>
                <property name="smfscancon:subtype">
                    <title>Sub Content Type</title>
                    <type>d:text</type>
                    <index enabled="true">
                        <tokenised>TRUE</tokenised>
                        <facetable>false</facetable>
                    </index>
                    <constraints>
                  		<constraint ref="smfscancon:subContentTypeList" />
               		</constraints>
                </property>
            </properties>
        </type>
    </types>
</model>