<?xml version="1.0" encoding="UTF-8"?>
<model name="witgp:contentModel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

    <!-- Optional meta-data about the model -->
    <description>Content model for GNMA DAMS</description>
    <author>Cyrus Munshi</author>
    <version>1.0</version>

    <imports>
        <!-- Import Alfresco Dictionary Definitions -->
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <!-- Import Alfresco Content Domain Model Definitions -->
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
        <!-- Import Alfresco System Model Definitions -->
        <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>
        <!-- Import Root GNMA Model -->
        <import uri="http://www.ginniemae.org/model/content/gnmaRoot/1.0" prefix="gnmaRoot"/>
    </imports>
    
    <namespaces>
        <namespace uri="http://www.ginniemae.org/model/content/witgp/1.0" prefix="witgp"/>
    </namespaces>

    <types>
        <type name="witgp:gpDocument">
            <title>OMO Workforce Innovation Team Ginnie Post (Newsletter) Documents</title>
            <parent>gnmaRoot:document</parent>
            <properties>
                <property name="witgp:subtype">
                    <title>GP Document Subtype</title>
                    <type>d:text</type>
                    <index enabled="true">
                        <tokenised>true</tokenised>
                        <facetable>true</facetable>
                    </index>
                </property>
                <property name="witgp:releaseDistributionDate">
                    <title>Release/Distribution Date</title>
                    <type>d:text</type>
	                <mandatory>true</mandatory>
                    <index enabled="true">
                        <tokenised>TRUE</tokenised>
                        <facetable>false</facetable>
                    </index>
                </property>
            </properties>
        </type>
                
        <type name="witgp:biweeklyEdition">
            <title>OMO Workforce Innovation Team GP Biweekly Edition</title>
            <parent>witgp:gpDocument</parent>
            <overrides>
                <property name="witgp:subtype">
                    <default>Biweekly Edition</default>
                </property>
            </overrides>
        </type>
        
    </types>

</model>