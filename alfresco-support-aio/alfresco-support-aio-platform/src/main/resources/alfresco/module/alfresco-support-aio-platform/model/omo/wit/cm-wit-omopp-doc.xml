<?xml version="1.0" encoding="UTF-8"?>
<model name="witomopp:contentModel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

    <!-- Optional meta-data about the model -->
    <description>Content model for GNMA DAMS</description>
    <author>Cyrus Munshi</author>
    <version>1.0</version>

    <imports>
        <!-- Import Alfresco Dictionary Definitions -->
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <!-- Import Alfresco Content Domain Model Definitions -->
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
        <!-- Import Alfresco System Model Definitions -->
        <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>
        <!-- Import Root GNMA Model -->
        <import uri="http://www.ginniemae.org/model/content/gnmaRoot/1.0" prefix="gnmaRoot"/>
    </imports>
    
    <namespaces>
        <namespace uri="http://www.ginniemae.org/model/content/witomopp/1.0" prefix="witomopp"/>
    </namespaces>

    <types>
        <type name="witomopp:omoppDocument">
            <title>OMO WIT OMO Performance Plan Documents</title>
            <parent>gnmaRoot:document</parent>
            <properties>
                <property name="witomopp:subtype">
                    <title>OMO Performance Plan Document Subtype</title>
                    <type>d:text</type>
                    <index enabled="true">
                        <tokenised>true</tokenised>
                        <facetable>true</facetable>
                    </index>
                </property>
                <property name="witomopp:fiscalYear">
                    <title>Fiscal Year</title>
                    <type>d:text</type>
	                <mandatory>true</mandatory>
                    <index enabled="true">
                        <tokenised>TRUE</tokenised>
                        <facetable>false</facetable>
                    </index>
                </property>
            </properties>
        </type>
                
        <type name="witomopp:annualPerformanceFinal">
            <title>OMO WIT Annual Performance Final (OMO)</title>
            <parent>witomopp:omoppDocument</parent>
            <overrides>
                <property name="witomopp:subtype">
                    <default>Annual Performance Final (OMO)</default>
                </property>
            </overrides>
        </type>
        
    </types>

</model>